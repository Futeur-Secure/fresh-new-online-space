{
  "rewrites": [
    { 
      "source": "/(.*)",
      "destination": "/use-cases/futeurcred",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/use-cases/futeurcred",
      "destination": "https://futeurcred.futeursecure.com",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.futeursecure.com"
        }
      ]
    },
    {
      "source": "/use-cases/futeurcred",
      "destination": "https://futeurcred.futeursecure.com",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "futeursecure.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ],
      "headers": [
        {
          "key": "x-matched-path",
          "value": "/"
        }
      ]
    }
  ],
  "trailingSlash": false
}
