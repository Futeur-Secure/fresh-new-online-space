{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/use-cases/futeurcred",
      "headers": {
        "x-matched-path": "/"
      },
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    }
  ],
  "trailingSlash": false
}
