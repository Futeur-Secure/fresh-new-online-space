{
  "rewrites": [
    { 
      "source": "/(.*)",
      "destination": "/use-cases/futeurcred",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "futeurcred.futeursecure.com"
        }
      ],
      "headers": [
        {
          "key": "x-matched-path",
          "value": "/"
        }
      ]
    }
  ],
  "trailingSlash": false
}
